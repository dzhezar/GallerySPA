    <!-- Services -->
    <section class="content-section bg-primary text-white text-center" id="services">
        <div class="container">
            <div class="content-section-heading">
                <h3 class="text-secondary mb-0">Services</h3>
                <h2 class="mb-5">What We Offer</h2>
            </div>
            <div class="row">
                <div class="col-lg-3 col-md-6 mb-5 mb-lg-0">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="icon-screen-smartphone"></i>
          </span>
                    <h4>
                        <strong>Responsive</strong>
                    </h4>
                    <p class="text-faded mb-0">Looks great on any screen size!</p>
                </div>
                <div class="col-lg-3 col-md-6 mb-5 mb-lg-0">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="icon-pencil"></i>
          </span>
                    <h4>
                        <strong>Redesigned</strong>
                    </h4>
                    <p class="text-faded mb-0">Freshly redesigned for Bootstrap 4.</p>
                </div>
                <div class="col-lg-3 col-md-6 mb-5 mb-md-0">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="icon-like"></i>
          </span>
                    <h4>
                        <strong>Favorited</strong>
                    </h4>
                    <p class="text-faded mb-0">Millions of users
                        <i class="fas fa-heart"></i>
                        Start Bootstrap!</p>
                </div>
                <div class="col-lg-3 col-md-6">
          <span class="service-icon rounded-circle mx-auto mb-3">
            <i class="icon-mustache"></i>
          </span>
                    <h4>
                        <strong>Question</strong>
                    </h4>
                    <p class="text-faded mb-0">I mustache you a question...</p>
                </div>
            </div>
        </div>
    </section>

    <!-- Portfolio -->
    <section class="content-section" id="portfolio">
        <div class="container">
            <div class="content-section-heading text-center">
                <h3 class="text-secondary mb-0">Portfolio</h3>
                <h2 class="mb-5">Recent Projects</h2>
            </div>
            <div class="row no-gutters">
                {% for photoshoot in mainPhotoshoots %}
                <div class="col-lg-6">
                    <a class="portfolio-item" data-fancybox data-src="#portfolio-item-{{ photoshoot.id }}" href="javascript:;">
                    <span class="caption">
                      <span class="caption-content">
                        <h2>{{ photoshoot.title }}</h2>
                        <p class="mb-0">{{ photoshoot.description | slice(0,70) }}</p>
                      </span>
                    </span>
                        <img class="img-portfolio" src="{{ photoshoot.images.shift.image }}" alt="">
                    </a>
                    <div style="display: none" id="portfolio-item-{{ photoshoot.id }}">
                        <div class="page-title photoshoot-title">
                            <h2>{{ photoshoot.title | title}}</h2>
                        </div>
                        <div class="photoshoot-text">
                            {{ photoshoot.description }}
                        </div>
                        <div>
                            <a href="{{ photoshoot.images.shift.image }}" data-fancybox="gallery-{{ photoshoot.id }}">
                                <img class="img-photoshoot" src="{{ photoshoot.images.shift.image }}" alt="">
                            </a>
                        </div>
                        <div class="photoshoot-description">
                            Model:{{ photoshoot.model }}<br>
                            Photographer: {{ photoshoot.photographer }}
                        </div>
                        <div class="container-custom">
                            {% for image in photoshoot.images %}
                                <a href="{{ image.image }}" data-fancybox="gallery-{{ photoshoot.id }}">
                                    <img class="img-photoshoot" src="{{ image.image }}" alt="">
                                </a>
                            {% endfor %}
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>
            <a class="btn btn-primary btn-xl mt-2 offset-5" href="{{ path('portfolio') }}">Find out more</a>
        </div>
        </div>
    </section>
    <!-- Map -->
    <section id="contact" class="map">
        <iframe width="100%" height="100%" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJWTUjZY3MHkcR2U7HZ_LJC-s&key=AIzaSyB8gROgli9JUpM85XaFCC7zRoYoPRKMkls" allowfullscreen></iframe>
        <br />
    </section>
{% endblock %}
