            <h2 class="mb-5">Recent Projects</h2>
        </div>
        <div class="row no-gutters">
            {% for photoshoot in mainPhotoshoots %}
            <div class="col-lg-6">
                <a class="portfolio-item" href="#">
                    <span class="caption">
                      <span class="caption-content">
                        <h2>{{ photoshoot.title }}</h2>
                        <p class="mb-0">{{ photoshoot.description | slice(0,70) }}</p>
                      </span>
                    </span>
                    <img class="img-fluid" src="{{ photoshoot.images.shift.image }}" alt="">
                </a>
            </div>
            {% endfor %}
        </div>
    </div>
</section>



